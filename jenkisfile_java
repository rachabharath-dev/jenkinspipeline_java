node{
   stage('Checkout'){
        checkout scm
   }
   stage('Build'){
        sh 'mvn clean'
   }
   stage('Compile'){
        sh 'mvn compile'
   }
   stage('Test'){
        sh 'mvn test'
   }
   stage('Package'){
        sh 'mvn package'
   }
  stage('Archive Artifacts'){
       archiveArtifacts artifacts:'target/*.jar',fingerprint:true
   }
  stage('Run') {
        sh 'java -cp target/hello-world-java-1.0-SNAPSHOT.jar com.example.App'
  }
  stage('success')
      echo "building compilation and testing compeleted succesfully"
  }   
}
